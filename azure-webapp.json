{
  "name": "DeepTrace",
  "runtime": "python",
  "runtimeVersion": "3.11",
  "startupCommand": "gunicorn wsgi:app --bind=0.0.0.0:8000 --timeout 600",
  "appSettings": [
    {
      "name": "FLASK_ENV",
      "value": "production"
    },
    {
      "name": "SCM_DO_BUILD_DURING_DEPLOYMENT",
      "value": "true"
    },
    {
      "name": "WEBSITE_HTTPLOGGING_RETENTION_DAYS",
      "value": "7"
    },
    {
      "name": "CARL_API_URL",
      "value": "https://ai.baytides.org/api/generate"
    },
    {
      "name": "CARL_DEFAULT_MODEL",
      "value": "qwen2.5:3b-instruct"
    }
  ],
  "linuxFxVersion": "PYTHON|3.11"
}
